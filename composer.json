{
  "name": "larva/think-settings",
  "description": "This is think settings module.",
  "keywords": [ "laravel", "settings", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "topthink/framework": "^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "larva\\settings\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "think": {
      "services": [
        "larva\\settings\\SettingsService"
      ],
      "aliases": {
        "Settings": "larva\\settings\\facade\\Settings"
      }
    }
  },
  "prefer-stable": true
}
